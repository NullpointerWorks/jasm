.def EXIT 0
.def OUT_A 1
.def OUT_B 2

jmp main

; ======================================================
; multiply
; ======================================================
m_multiply:
	push a
	inc sp
	load a, &sp
	int OUT_A
	
	
	
	
	dec sp
	pop a
