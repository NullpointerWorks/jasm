jmp main

; ===========================
; multiply method
; ===========================
multiply:
	cmp a,0
	je _mul_done
_mul_loop:
	add c,b
	dec a
	je _mul_done
	jmp _mul_loop
_mul_done:
	load a,c
	ret


; ===========================
; program main
; ===========================
main:
	load a,7
	load b,7
	call multiply
	int 10
	int 0
	